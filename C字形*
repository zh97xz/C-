#include<stdio.h>

int main()
{
	int x = 0;
	int y = 0;
	
		
			printf(" ");
			for (y = 1; y < 4; y++)
			{
				printf("*");
			}
			printf("\n");
		for (x = 1; x < 4; x++)
		{
			
			printf("*\n");
		}
		if (x == 4)
		{
			printf(" ");
			for (y = 1; y < 4; y++)
			{
				printf("*");
			}
		}
	

	return 0;
