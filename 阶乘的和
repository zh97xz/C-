#include<stdio.h>

long double jc(int j)
{
	int m = 0;
	long double chengji = 1;
	for (m = 1; m <= j; m++)
	{
		chengji *= m;

	}
	return chengji;
}



int main()
{
	long double jc(int j);
	int i = 0;
	long double sum = 0;
	for (i = 1; i <= 20; i++)
	{
		sum += jc(i);
	}
	printf("sum=%lf",sum);
	return 0;
}
